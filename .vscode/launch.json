{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "CMake Debug (MinGW gdb)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${command:cmake.launchTargetPath}",
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb",
      "miDebuggerPath": "C:\\msys64\\ucrt64\\bin\\gdb.exe",
      "console": "integratedTerminal",
      "environment": [
        { "name": "PATH", "value": "C:\\msys64\\ucrt64\\bin;${env:PATH}" }
      ],
      "stopAtEntry": false
    }
  ]
}
